{
  "kuzzle-plugin-logger": {
    "version": "1.0.6",
    "activated": true,
    "defaultConfig": {
      "service": "winston",
      "level": "info",
      "addDate": true
    }
  },
  "kuzzle-plugin-socketio": {
    "version": "1.0.3",
